<launch>

	<node name="node_simulation" pkg="sailboat" type="simu_sailboat" />
	<node name="node_draw_waypoint" pkg="sailboat" type="drawwaypoint" />
	<param name="boat_x" type="double" value="20" />
	<param name="boat_y" type="double" value="-30.0" />
	<param name="psi_wind" type="double" value="1.5" />
	<param name="boat_head" type="double" value="0" />


	<node name="node_circle" pkg="sailboat" type="circle" />

	<param name="SKx" type="double" value="-35" />
	<param name="SKy" type="double" value="10.0" />
	<param name="radius" type="double" value="30.0" />
	
</launch>
